class Solution {
    public:
    vector<int> preorder(Node* root) {
        if(!root) return {};
        
        vector<int> ans;
        stack<Node*> stk;
        stk.push(root);
        
        while(stk.size()){
            Node *curr = stk.top(); stk.pop();
            ans.push_back(curr->val);
            
            for(int i=curr->children.size()-1; i>=0; i--){
                if(curr->children[i]){
                    stk.push(curr->children[i]);
                }
            }
        }
        
        return ans;
    }
};